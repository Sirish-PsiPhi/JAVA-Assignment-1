import java.util.Scanner;
public class Exercise1_4 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		   int n=sc.nextInt();
	           int result=0;
	           
	           int num,remainder,sum=0;
	           num = n;
	           
	            while(n>0) {
	        	   remainder = n % 10;	     
	        	   sum = sum + (remainder * remainder *remainder);
	        	   n = n/10;	        	  
	            }
	            
	           	  if(sum == num) {
	        	   result = 1;
	           }
	           System.out.print(result);
	}

}
